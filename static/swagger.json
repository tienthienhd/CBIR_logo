          "multipart/form-data"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "formData",
            "name": "image",
            "description": "image to check ",
            "required": true,
            "type": "file"
          },
          {
            "in": "formData",
            "name": "label",
            "description": "type of logo",
            "default": "pepsi",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "type": "object",
              "properties": {
                "status_code": {
                  "type": "integer",
                  "format": "int32"
                },
                "message": {
                  "type": "string"
                },
                "has_logo": {
                  "type": "boolean"
                }
              }
            }
          }
        }
      }
